{
  "name": "love2d-typescript",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Jake Klassen",
  "license": "MIT",
  "type": "module",
  "imports": {
    "#app/*": "./dist/*"
  },
  "scripts": {
    "prebuild": "shx rm -rf game",
    "build": "shx rm -rf game && pnpm build:tstl && pnpm build:copy",
    "build:tstl": "tstl -p tsconfig.build.json",
    "build:copy": "shx cp -R res game",
    "build:watch": "tstl -w -p tsconfig.build.json",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" .eslintrc.cjs",
    "start": "love game --console",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/eslint": "8.40.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "comment-json": "4.2.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "expect": "29.5.0",
    "love-typescript-definitions": "^0.24.3",
    "lua-types": "^2.13.1",
    "prettier": "2.8.8",
    "shx": "0.3.4",
    "tsx": "^3.12.7",
    "typescript": "5.1.3",
    "typescript-to-lua": "^1.16.2",
    "vitest": "^0.32.1"
  }
}
